{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es3",
    "outDir": "wwwroot/js",
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]

}
